{
  "manifest_version": 2,

  "name": "EECS499: Independent Study",
  "description": "Can we limit your socal media usage?",
  "version": "1.7",
  "author": "joshuahs@umich.edu",

  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "blockm16.png",
    "48": "blockm48.png",
    "128": "blockm128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "blockm19.png",
      "38": "blockm38.png"
    }
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
      ],
    "persistent": true
  },
  "options_page": "options.html"
}
